<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        jcr:mixinTypes="[sling:VanityPath]"
        jcr:primaryType="nt:unstructured"
        jcr:title="Reports"
        sling:resourceType="granite/ui/components/shell/collectionpage"
        sling:vanityOrder="{Long}1301"
        sling:vanityPath="/communities/reports"
        consoleId="cq-communities-reports"
        currentView="content"
        targetCollection="coral-masonry">
        <head jcr:primaryType="nt:unstructured">
            <clientlibs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/includeclientlibs"
                categories="[coralui3]"/>
        </head>
        <actions
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/actionbar"/>
        <breadcrumbs
            jcr:primaryType="nt:unstructured"
            sling:resourceType="/libs/social/console/components/datasources/breadcrumbs"
            path="/libs/social/reporting/content-shell3/reports"
            rootPath="/libs/social/reporting/content-shell3/reports"/>
        <views jcr:primaryType="nt:unstructured">
            <content
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <reportlist
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="social/reporting/components/hbs/reportcardlist"/>
                </items>
            </content>
        </views>
    </jcr:content>
    <assignments/>
    <posts/>
    <views/>
</jcr:root>
