<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:rep="internal"
    jcr:mixinTypes="[rep:AccessControllable]"
    jcr:primaryType="nt:unstructured"
    jcr:title="Communities"
    id="cq-communities">
    <rep:policy/>
    <admin
        jcr:primaryType="nt:unstructured"
        jcr:title="Administration"
        sling:orderBefore="resources"
        id="cq-communities-admin">
        <siteblueprints
            jcr:primaryType="nt:unstructured"
            jcr:title="Site Templates"
            href="/communities/communitysitetemplates"
            id="cq-communities-siteblueprints"/>
        <groupblueprints
            jcr:primaryType="nt:unstructured"
            jcr:title="Group Templates"
            href="/communities/communitygrouptemplates"
            id="cq-communities-groupblueprints"/>
        <functionalblueprints
            jcr:primaryType="nt:unstructured"
            jcr:title="Community Functions"
            href="/communities/communityfunctions"
            id="cq-communities-functionalblueprints"/>
        <storageconfiguration
            jcr:primaryType="nt:unstructured"
            jcr:title="Storage Configuration"
            href="/communities/admin/defaultsrp"
            id="cq-communities-defaultsrp"/>
        <categories
            jcr:primaryType="nt:unstructured"
            jcr:title="Tagging"
            href="/aem/tags.html/etc/tags"
            id="cq-communities-categories"/>
    </admin>
    <security
        jcr:primaryType="nt:unstructured"
        jcr:title="Security"
        sling:orderBefore="admin"
        id="cq-communities-security">
        <members
            jcr:primaryType="nt:unstructured"
            jcr:title="Members"
            href="/communities/members"
            id="cq-communities-users"/>
        <groups
            jcr:primaryType="nt:unstructured"
            jcr:title="Groups"
            href="/communities/groups"
            id="cq-communities-groups"/>
    </security>
    <moderation
        jcr:primaryType="nt:unstructured"
        jcr:title="Moderation"
        sling:orderBefore="security"
        href="/communities/moderation"
        id="cq-communities-moderation"/>
    <sites
        jcr:primaryType="nt:unstructured"
        jcr:title="Sites"
        sling:orderBefore="moderation"
        href="/communities/sites"
        id="cq-communities-sites"/>
    <reports
        jcr:mixinTypes="[granite:InternalArea]"
        jcr:primaryType="nt:unstructured"
        jcr:title="Reports"
        id="cq-communities-reports">
        <views
            jcr:mixinTypes="[granite:InternalArea]"
            jcr:primaryType="nt:unstructured"
            jcr:title="Views"
            href="/communities/reports/views"
            id="cq-communities-reports-views"/>
        <posts
            jcr:mixinTypes="[granite:InternalArea]"
            jcr:primaryType="nt:unstructured"
            jcr:title="Posts"
            href="/communities/reports/posts"
            id="cq-communities-reports-posts"/>
        <assignments
            jcr:primaryType="nt:unstructured"
            jcr:title="Assignments"
            href="/communities/reports/assignments"
            id="cq-communities-reports-assignments"/>
    </reports>
    <resources
        jcr:primaryType="nt:unstructured"
        jcr:title="Resources"
        href="/communities/resources.html"
        id="cq-communities-resources"/>
</jcr:root>
