<div class="scf scf-user-profile-top-badges" data-component-id="{{id}}" data-scf-component="social/console/components/hbs/userbadges">
    <div class="scf-js-user-profile-badges-top">
        {{#each earnedTopBadges}}
            <span class="scf-js-user-profile-top-earned-badge"><img src="{{this.imageUrl}}" /></span>
        {{/each}}
        <span class="scf-js-user-profile-top-earned-badge-count"></span>
    </div>
    <div class="scf-js-user-profile-badges-top">
        {{#each assignedBadges}}
            <span class="scf-js-user-profile-top-assigned-badge"><img src="{{this.imageUrl}}" /></span>
        {{/each}}
        <span class="scf-js-user-profile-top-assigned-badge-count"></span>
    </div>
</div>
