{{#each structure}}
    <li coral-dragaction data-function-path="{{function.id}}" class="scf-is-list-added-item" data-component-type="{{function.componentType}}" data-function-url="{{url}}" data-configuration="{{configuration}}">
        <i class="coral-Icon coral-Icon--{{function.iconClass}} scf-console-icon-display"></i>
        <span class="scf-func-title-grp">
                <span class="scf-is-func-title">{{i18n title}}</span>
        <span class="scf-func-name">{{i18n function.title}}</span>
        </span>
        <i class="coral-Icon coral-Icon--apps scf-console-list-move"></i>
        <i class="coral-Icon coral-Icon--delete scf-console-list-item-icon scf-js-list-remove scf-js-item-action"></i>
        <i class="coral-Icon coral-Icon--gear scf-console-list-item-icon scf-js-list-edit scf-js-item-action"></i>
    </li>
{{/each}}
