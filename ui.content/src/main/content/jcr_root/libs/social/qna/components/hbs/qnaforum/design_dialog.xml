<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:mixinTypes="[granite:InternalArea]"
    jcr:primaryType="cq:Dialog"
    title="Subtypes"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Resource Types">
                <items jcr:primaryType="cq:WidgetCollection">
                    <topictype
                        jcr:primaryType="nt:unstructured"
                        allowBlank="true"
                        fieldLabel="Topic Resource Type"
                        name="./qnatopicresourcetype"
                        predicate="nosystem"
                        rootPath="/apps"
                        showTitlesInTree="false"
                        xtype="pathfield"/>
                    <posttype
                        jcr:primaryType="nt:unstructured"
                        allowBlank="true"
                        fieldLabel="Post Resource Type"
                        name="./qnapostresourcetype"
                        predicate="nosystem"
                        rootPath="/apps"
                        showTitlesInTree="false"
                        xtype="pathfield"/>
                    <votingtype
                        jcr:primaryType="nt:unstructured"
                        allowBlank="true"
                        fieldLabel="Voting Resource Type"
                        name="./votingresourcetype"
                        predicate="nosystem"
                        rootPath="/apps"
                        showTitlesInTree="false"
                        xtype="pathfield"/>
                    <votingRoot
                        jcr:primaryType="cq:Widget"
                        ignoreData="true"
                        name="./votingRoot"
                        value="voting"
                        xtype="hidden"/>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
