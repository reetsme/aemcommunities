<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        jcr:mixinTypes="[sling:VanityPath]"
        jcr:primaryType="nt:unstructured"
        jcr:title="AEM Community Groups"
        sling:resourceType="granite/ui/components/foundation/page"
        sling:vanityOrder="{Long}301"
        sling:vanityPath="/communities/groups">
        <redirector
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/admin/contentchecker"
            allowEmpty="{Boolean}false"
            path="/content"
            redirect="/communities/groups.html"/>
        <head jcr:primaryType="nt:unstructured">
            <viewport
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/admin/page/viewport"/>
            <meta
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/meta"
                content="chrome=1"
                name="X-UA-Compatible"/>
            <title
                jcr:primaryType="nt:unstructured"
                sling:resourceType="cq/gui/components/siteadmin/admin/page/title"/>
            <favicon
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/page/favicon"/>
            <clientlibs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/includeclientlibs"
                css="[coralui3,coralui2,granite.ui.foundation,cq.social.members.hbs.shell2,cq.social.members.hbs.groups]"
                js="[coralui3,granite.ui.foundation,granite.ui.coral.foundation,cq.social.author.coral3.shim,cq.social.members.hbs.shell2,cq.social.members.hbs.groups]"/>
        </head>
        <body
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/page/body">
            <layout
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/endor/layouts/page"/>
            <rail
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/endor/navcolumns"
                currentId="cq-communities-groups"
                srcPath="cq/core/content/nav"/>
            <breadcrumbs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="cq/gui/components/siteadmin/admin/endor/breadcrumbs"
                baseUrl="/communities/groups.html"
                contentPath="${requestPathInfo.suffix}"
                depthAdjustment="{Long}-2"
                rootPath=""
                rootTitle="Communities - Groups"/>
            <title
                jcr:primaryType="nt:unstructured"
                sling:resourceType="cq/gui/components/siteadmin/admin/endor/title"
                contentPath="/content"
                rootTitle="Communities - Groups"/>
            <header
                jcr:primaryType="nt:unstructured"
                srcPath="cq/core/content/header"/>
            <content
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/endor/layouts/content"/>
                <header
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/container">
                    <layout
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/layouts/mode"
                        group="cq-social-enablement"/>
                    <items jcr:primaryType="nt:unstructured">
                        <default
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/navigation">
                            <layout
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/endor/layouts/actionbar"/>
                            <items jcr:primaryType="nt:unstructured">
                                <search
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/endor/actionbar/innerrailtoggle"
                                    icon="icon-search"
                                    targetId="granite-rail-filter"
                                    text="Search"
                                    title="Search"/>
                                <create_visible
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/endor/actionbar/hyperlink"
                                    href="/communities/groupprofile.html?groupId="
                                    text="Create"
                                    variant="primary">
                                    <layoutConfig
                                        jcr:primaryType="nt:unstructured"
                                        align="right"/>
                                </create_visible>
                            </items>
                        </default>
                    </items>
                </header>
                <content
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/container">
                    <items jcr:primaryType="nt:unstructured">
                        <preview
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Preview"
                            sling:resourceType="granite/ui/components/foundation/container"
                            class="u-coral-hidden">
                            <items jcr:primaryType="nt:unstructured">
                                <groups
                                    granite:rel="cq-community-groups"
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="social/members/components/hbs/groups"
                                    fromPublisher="true"
                                    modeGroup="cq-community-groups"
                                    pageSize="40"
                                    path="${requestPathInfo.suffix}"/>
                            </items>
                        </preview>
                    </items>
                </content>
                <aside
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/container">
                    <layout
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/endor/layouts/sidepanels"/>
                    <items jcr:primaryType="nt:unstructured">
                        <filter
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/container"
                            id="granite-rail-filter">
                            <items jcr:primaryType="nt:unstructured">
                                <searchform
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/form"
                                    action="/libs/social/members/content/groups/jcr:content/body/content/content/items/preview/items/groups.social.json?_charset_=utf-8"
                                    async="{Boolean}true"
                                    autosubmitForm="{Boolean}true"
                                    foundationForm="{Boolean}true">
                                    <items jcr:primaryType="nt:unstructured">
                                        <user
                                            jcr:primaryType="nt:unstructured"
                                            jcr:title="Keyword"
                                            sling:resourceType="granite/ui/components/foundation/container">
                                            <layout
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/layouts/collapsible"
                                                variant="block"/>
                                            <items jcr:primaryType="nt:unstructured">
                                                <keywordinput
                                                    granite:class="scf-js-members-keyword-textbox"
                                                    jcr:primaryType="nt:unstructured"
                                                    jcr:title="Keyword"
                                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                    emptyText="Search"
                                                    name="keyword"/>
                                            </items>
                                        </user>
                                        <site
                                            jcr:primaryType="nt:unstructured"
                                            jcr:title="Site"
                                            sling:resourceType="granite/ui/components/foundation/container">
                                            <layout
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/layouts/collapsible"
                                                variant="block"/>
                                            <items jcr:primaryType="nt:unstructured">
                                                <sitefilter
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceSuperType="social/console/components/hbs/sitecollection"
                                                    sling:resourceType="social/members/components/hbs/groups/filters/sitefilter"/>
                                            </items>
                                        </site>
                                        <controlscontainer
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/container"
                                            id="filter-controls-container">
                                            <items jcr:primaryType="nt:unstructured">
                                                <filtercontrols
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="social/members/components/hbs/groups/filters/controls"/>
                                            </items>
                                        </controlscontainer>
                                    </items>
                                </searchform>
                            </items>
                        </filter>
                    </items>
                </aside>
            </content>
        </body>
    </jcr:content>
</jcr:root>
