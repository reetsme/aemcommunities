<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:description="A simple fragment, containing one single element and no predefined variations"
    jcr:mixinTypes="[granite:InternalArea]"
    jcr:primaryType="nt:unstructured"
    jcr:title="Simple Fragment"
    precreateElements="{Boolean}true"
    version="{Long}1">
    <elements jcr:primaryType="nt:unstructured">
        <main
            jcr:primaryType="nt:unstructured"
            jcr:title="Main"
            defaultContent=""
            name="main"/>
    </elements>
</jcr:root>
