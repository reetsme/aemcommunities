<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:mixinTypes="[granite:InternalArea]"
    jcr:primaryType="cq:Page">
    <jcr:content
        jcr:mixinTypes="[sling:VanityPath]"
        jcr:primaryType="nt:unstructured"
        jcr:title="AEM Communities Moderation"
        sling:resourceType="granite/ui/components/foundation/page"
        sling:vanityOrder="{Long}301"
        sling:vanityPath="/communities/moderation">
        <head jcr:primaryType="nt:unstructured">
            <viewport
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/admin/page/viewport"/>
            <meta
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/meta"
                content="chrome=1"
                name="X-UA-Compatible"/>
            <title
                jcr:primaryType="nt:unstructured"
                sling:resourceType="cq/gui/components/siteadmin/admin/page/title"/>
            <favicon
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/page/favicon"/>
            <clientlibs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/includeclientlibs"
                css="[coralui3,coralui2,cq.ui.social.core.communities.filters]"
                js="[coralui3,granite.ui.coral.foundation,coralui2,granite.ui.foundation,granite.ui.foundation.admin,cq.common.wcm,cq.siteadmin.admin,cq.ui.social.core.communities.filters]"/>
        </head>
        <body
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/page/body">
            <layout
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/endor/layouts/page"
                class="cq-communities-moderation-page"/>
            <rail
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/endor/navcolumns"
                currentId="cq-communities-moderation"
                srcPath="cq/core/content/nav"/>
            <breadcrumbs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="cq/gui/components/siteadmin/admin/endor/breadcrumbs"
                baseUrl="/communities/moderation.html"
                contentPath="${requestPathInfo.suffix}"
                depthAdjustment="{Long}-1"
                rootPath=""
                rootTitle="Moderation"/>
            <title
                jcr:primaryType="nt:unstructured"
                sling:resourceType="cq/gui/components/siteadmin/admin/endor/title"
                contentPath="/content"
                rootTitle="Communities - Moderation"/>
            <header
                jcr:primaryType="nt:unstructured"
                srcPath="cq/core/content/header"/>
            <content
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/endor/layouts/content"/>
                <content
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/container"
                    class="cq-communities-moderation-scrollpanel">
                    <items jcr:primaryType="nt:unstructured">
                        <modcontainer
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="social/moderation/modcontainer">
                            <filters
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="social/moderation/filters"/>
                            <ugc
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="social/moderation/dashboard"/>
                            <userdetails
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="social/moderation/userdetails"/>
                            <userlist
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="social/console/components/hbs/simpleusers"
                                fromPublisher="true"
                                groupId="community-members"
                                includeSubGroups="true"
                                type="simpleusers"/>
                            <activitystream
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="social/activitystreams/components/hbs/activitystreams"
                                allowAllView="true"
                                allowFollowingView="false"
                                allowUserView="false"
                                streamContextPath="/content"
                                streamName="moderation"/>
                        </modcontainer>
                    </items>
                </content>
            </content>
        </body>
    </jcr:content>
    <loaders jcr:primaryType="nt:unstructured">
        <authordetail
            jcr:primaryType="nt:unstructured"
            sling:resourceType="social/core/components/communities/authordetail"/>
        <activitystream
            jcr:primaryType="nt:unstructured"
            sling:resourceType="social/core/components/communities/activitystream"/>
    </loaders>
</jcr:root>
