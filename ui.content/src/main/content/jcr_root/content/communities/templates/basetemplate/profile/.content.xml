<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:scg="http://www.adobe.com/social/scg/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:mix="http://www.jcp.org/jcr/mix/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:deviceGroups="[mobile/groups/responsive]"
        jcr:isCheckedOut="{Boolean}true"
        jcr:mixinTypes="[mix:versionable]"
        jcr:primaryType="cq:PageContent"
        jcr:title="Profile"
        jcr:uuid="0ef609f2-1618-45aa-a7de-47290f90f8a5"
        sling:resourceType="social/console/components/profilepage"
        hideInNav="{Boolean}true"
        navTitle="Profile">
        <clientlibslist
            jcr:primaryType="nt:unstructured"
            scg:requiredClientLibs="[cq.social.hbs.breadcrumbs,cq.social.hbs.userprofile,cq.social.hbs.forum,personalization.core.kernel,personalization.clientcontext.kernel,personalization.stores.kernel,personalization.kernel]"
            sling:resourceType="social/console/components/clientlibslist"/>
        <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="social/console/components/hbs/rightcolumncontent">
            <breadcrumbs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="social/console/components/hbs/breadcrumbs"
                maxFileSize="104857600"
                maxImageFileSize="2097152"
                maxMessageLength="104857600"
                maxSubjectLength="104857600"
                rteEnabled="true"/>
            <primary
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid">
                <userprofile
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="social/console/components/hbs/userprofile">
                    <activitystream
                        jcr:created="{Date}2014-01-09T12:45:08.070-05:00"
                        jcr:createdBy="admin"
                        jcr:lastModified="{Date}2014-10-24T15:53:14.735-07:00"
                        jcr:lastModifiedBy="admin"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="social/activitystreams/components/hbs/activitystreams"
                        allowUserView="true"
                        name="Activity"
                        title="Activity"/>
                    <userbadges
                        jcr:created="{Date}2014-01-09T12:45:08.070-05:00"
                        jcr:createdBy="admin"
                        jcr:lastModified="{Date}2014-10-24T15:53:14.735-07:00"
                        jcr:lastModifiedBy="admin"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="social/console/components/hbs/userbadges"
                        allowUserView="true"
                        name="Badges"
                        title="Badges"/>
                </userprofile>
            </primary>
            <auxilary
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid">
                <followees
                    cq:lastRolledout="{Date}2015-03-22T16:40:29.362-07:00"
                    cq:lastRolledoutBy="admin"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="social/socialgraph/components/hbs/relationships"
                    relType="USER"/>
                <followers
                    cq:lastRolledout="{Date}2015-03-22T16:40:29.362-07:00"
                    cq:lastRolledoutBy="admin"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="social/socialgraph/components/hbs/relationships"
                    outgoing="false"
                    relType="USER"/>
            </auxilary>
        </content>
        <cq:responsive jcr:primaryType="nt:unstructured">
            <breakpoints jcr:primaryType="nt:unstructured">
                <phone
                    jcr:primaryType="nt:unstructured"
                    title="Smaller Screen"
                    width="{Decimal}768"/>
                <tablet
                    jcr:primaryType="nt:unstructured"
                    title="Tablet"
                    width="{Decimal}992"/>
                <mediumscreen
                    jcr:primaryType="nt:unstructured"
                    title="Medium Screen"
                    width="{Decimal}1200"/>
            </breakpoints>
        </cq:responsive>
    </jcr:content>
</jcr:root>
